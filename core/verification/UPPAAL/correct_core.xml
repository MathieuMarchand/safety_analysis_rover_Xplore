	<system>// Place template instantiations here.
MC = manualCommands();
core = rover_core();
CS = control_station();
nav = navigation();
man = manual();
hd = maintenance();
sc = science();

// List one or more processes to be composed into a system.
system CS, core, MC, nav, man, hd, sc;
//system CS, core, nav, man, hd, sc;
</system>
	<queries>
		<option key="--diagnostic" value="1"/>
		<query>
			<formula/>
			<comment>------ Death verification -------</comment>
		</query>
		<query>
			<formula>A[] man.deathcount &gt; DEATH_THRESHOLD + 1 imply man.stopped ||man.faulty</formula>
			<comment>U1 : Manual stops if it doesn't receive a heartbeat from the core for too long</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:56:02 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] sc.deathcount &gt; DEATH_THRESHOLD + 1 imply sc.stopped || sc.faulty</formula>
			<comment>U1 : Science stops if it doesn't receive a heartbeat from the core for too long</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:56:40 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] hd.deathcount &gt; DEATH_THRESHOLD + 1 imply hd.stopped || hd.faulty</formula>
			<comment>U1 : Maintenance stops if it doesn't receive a heartbeat from the core for too long</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:57:16 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] nav.deathcount &gt; DEATH_THRESHOLD + 1 imply nav.stopped ||nav.faulty</formula>
			<comment>U1 : Navigation stops if it doesn't receive a heartbeat from the core for too long</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:57:53 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] man.launched imply man.heartbeat &lt;= DEATH_THRESHOLD</formula>
			<comment>U1 : Safety, if the module is launched, then the threshold has not been reached</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 11:09:43 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] CS.dead imply man.stopped ||man.heartbeat &lt;= DEATH_THRESHOLD</formula>
			<comment/>
			<result outcome="failure" type="quality" timestamp="2023-12-07 13:38:43 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] CS.dead &amp;&amp; core.fault imply man.stopped &amp;&amp; nav.stopped &amp;&amp; sc.stopped &amp;&amp; hd.stopped</formula>
			<comment>U2 : If the core is in fault state because it lost signal with the control station, all modules are stopped</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:29:58 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] core.CSConnected &amp;&amp; CS.dead &amp;&amp; core.deathcount &gt; DEATH_THRESHOLD + 1 imply core.fault || core.dead</formula>
			<comment>U2 : If the core has been connected and the control station died and the threshold has been reached, it is either dead or in fault state</comment>
			<option key="--diagnostic" value="1"/>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:17:15 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] not core.CSConnected imply man.stopped &amp;&amp; nav.stopped &amp;&amp; sc.stopped &amp;&amp; hd.stopped</formula>
			<comment>RXX : When the control station is not connected, no module is launched</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:45:49 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
		<query>
			<formula>A[] core.dead &amp;&amp; man.heartbeat &gt; DEATH_THRESHOLD + 1 imply man.stopped ||man.faulty</formula>
			<comment>U3 : If the core dies and the threshold has been reached, manual stops</comment>
			<result outcome="success" type="quality" timestamp="2023-12-07 13:40:35 +0100">
				<option key="--diagnostic" value="1"/>
			</result>
		</query>
	</queries>
